<Project ToolsVersion="15.0">
  <!-- Dependency versions -->
  <PropertyGroup>
    <NuGetPackageVersion>4.4.0</NuGetPackageVersion>
    <NuGetTestHelpersVersion>1.3.3</NuGetTestHelpersVersion>
    <CommandLineUtilsVersion>2.0.0</CommandLineUtilsVersion>
    <FileSystemGlobbingVersion>2.0.0</FileSystemGlobbingVersion>
  </PropertyGroup>
  
  <!-- Config -->
  <PropertyGroup>
    <RepositoryName>NupkgWrench</RepositoryName>
  </PropertyGroup>

  <!-- Nuspec defaults -->
  <PropertyGroup>
    <Authors>emgarten</Authors>
    <PackageProjectUrl>https://github.com/emgarten/NupkgWrench</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/emgarten/NupkgWrench/blob/master/LICENSE.md</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/emgarten/NupkgWrench</RepositoryUrl>
    <PackageTags>nupkg;nuget-powertool</PackageTags>
    <Description>NupkgWrench is a cross platform command line tool to read and modify nupkgs.</Description>
    <Copyright>Copyright Â© 2017 Justin Emgarten</Copyright>
    <PackageIconUrl>https://emgartenstatic.blob.core.windows.net/nupkgs/icons/nupkgwrench.png</PackageIconUrl>
  </PropertyGroup>
</Project>